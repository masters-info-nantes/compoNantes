module compoNantesToUml;
create OUT : UML from IN : COMPO;

helper context COMPO!Component def: providedInterfaces(): Collection(UML!Interface)=
	;
	
rule ComponanteToComponent {
	from
		cpn : COMPO!Component
	to 
		cn : UML!Component (
			name <- cpn.name,
			ownedPort <- cpn.ports,
			provided <- cpn,
			
		)
}