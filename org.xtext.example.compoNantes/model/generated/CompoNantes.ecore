        eType="#//Usage" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="Type">
    <eLiterals name="REQUIRED" literal="required"/>
    <eLiterals name="PROVIDED" value="1" literal="provided"/>
